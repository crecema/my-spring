<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="apiSpaceClient" class="com.crecema.my.spring.common.client.ApiSpaceClient"/>

    <bean id="jokesService" class="com.crecema.my.spring.common.service.JokesServiceImpl">
        <constructor-arg index="0" ref="apiSpaceClient"/>
    </bean>

    <bean id="jokesLauncher" class="com.crecema.my.spring.common.launcher.JokesLauncher">
        <constructor-arg index="0" ref="jokesService"/>
    </bean>

</beans>